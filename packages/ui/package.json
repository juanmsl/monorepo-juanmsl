{
  "name": "@juanmsl/ui",
  "version": "0.0.20",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "build": "tsc && tsup",
    "transform-svgs": "node ../../node_modules/@juanmsl/svg-to-react",
    "transform-svgs2": "node @juanmsl/svg-to-react",
    "publish-package": "yarn lint && yarn build && changeset version && changeset publish"
  },
  "devDependencies": {
    "@juanmsl/svg-to-react": "*",
    "@types/color": "^3.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.18",
    "@types/react-pdf": "^7.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^5.0.0",
    "framer-motion": "^11.0.24",
    "react": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "styled-components": "^5.3.11",
    "tsconfig": "*",
    "tsup": "^8.0.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "framer-motion": "^11",
    "react": "^18",
    "react-hook-form": "^7",
    "styled-components": "^5"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.3",
    "@hookform/resolvers": "^3.1.0",
    "axios": "^1.4.0",
    "color": "^4.2.3",
    "moment": "^2.30.1",
    "react-colorful": "^5.6.1",
    "react-pdf": "^7.7.1",
    "use-eye-dropper": "^1.6.1",
    "yup": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
