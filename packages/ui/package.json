{
  "name": "@juanmsl/ui",
  "version": "0.0.48",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "build:watch": "tsup --watch",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && tsup",
    "publish-package": "yarn lint && yarn build && yarn publish --no-git-tag-version",
    "transform-svgs": "node ../../node_modules/@juanmsl/svg-to-react && yarn lint:fix"
  },
  "devDependencies": {
    "@juanmsl/svg-to-react": "*",
    "@types/color": "^3.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.18",
    "@types/react-pdf": "^7.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-custom": "*",
    "eslint-plugin-prettier": "^5.0.0",
    "framer-motion": "^11.0.24",
    "react": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "styled-components": "^6.1.11",
    "tsconfig": "*",
    "tsup": "^8.0.1",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "framer-motion": "^11.0.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "styled-components": "^6.1.11"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@juanmsl/helpers": "0.0.5",
    "@juanmsl/hooks": "0.0.12",
    "axios": "^1.4.0",
    "color": "^4.2.3",
    "moment": "^2.30.1",
    "react-colorful": "^5.6.1",
    "react-pdf": "^7.7.1",
    "use-eye-dropper": "^1.6.1",
    "yup": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
